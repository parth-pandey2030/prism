{
    "tokens": {
        "identifier": "^[a-zA-Z_][a-zA-Z0-9_]*$",
        "int": "^[0-9]+$",
        "float": "^[0-9]+\\.[0-9]+$",
        "string": "^[a-zA-Z_][a-zA-Z0-9_]*$",
        "operator": "^[+\\-*/%&|^~<>!]+",
        "punctuation": "^[{}()[\\]\\.\\,;:]",
        "other": {
            "comment": "#//.*$",
            "whitespace": "^[ \t]+",
            "newLine": "^\n",
            "infinity": "inf",
            "null": "null"
        }
    }
}