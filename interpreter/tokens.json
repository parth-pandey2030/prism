{
    "tokens": {
        "identifier": "^[a-zA-Z_][a-zA-Z0-9_]*$",
        "int": "^[0-9]+$",
        "float": "^[0-9]+\\.[0-9]+$",
        "irrational": "^[0-9]+\\.[0-9]+[eE][+-]?[0-9]+$",
        "boolean": "^(true|false)$",
        "complex": "^[0-9]+\\.[0-9]+[iJ][+-]?[0-9]+$",
        "string": "^[a-zA-Z_][a-zA-Z0-9_]*$",
        "array": "^[\\[\\]\\,\\s]*$",
        "set": "^()$",
        "hash": "^{}$",
        "operator": "^[+\\-*/%&|^~<>!]+",
        "punctuation": "^[{}()[\\]\\.\\,;:]",
        "loop": {
            "forLoop": "for",
            "whileLoop": "while",
            "other": {
                "finishedLoop": "finish",
                "continueLoop": "continue",
                "breakLoop": "break"
            }
        },
        "other": {
            "comment": "^#//.*/*$",
            "whitespace": "^[ \t]+",
            "newLine": "^\\n",
            "infinity": "inf",
            "null": "null",
            "undefined": "undefined",
            "nan": "nan",
            "function": "func"
        }
    }
}